{
  "name": "liveany notify",
  "version": "1.0",
  "description":
    "Shows off desktop notifications when stranger reply you.",
  "icnos": {"64": "64.png", "128": "128.png"},
  "permissions": [
    "notifications",
    "http://www.liveany.com/*",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
      {
          "matches": ["http://www.liveany.com/*"],
          "js": ["jquery.js", "myscript.js"]
      }
  ],
  "manifest_version": 2
}
